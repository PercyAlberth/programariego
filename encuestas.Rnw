#cargar librerias
library(knitr)
library(kableExtra)
library(dplyr)
library(stringr)
library(ggplot2)

crear_tabla <- function(datos, columna, nuevo_nombre) {
#setwd("H:/Gore Cusco/Geragri/programa/analisis datos/analisis2/programariego")
#datos <- read.csv("H:/Gore Cusco/Geragri/programa/analisis datos/analisis2/programariego/base/encuestas.csv")  
  
  tabla <- datos %>%
    select(!!sym(columna)) %>%
    rename(!!nuevo_nombre := !!sym(columna)) %>%
    group_by(!!sym(nuevo_nombre)) %>%
    summarise(Conteo = n()) %>%
    arrange(!!sym(nuevo_nombre)) %>%
    mutate(Porcentaje = round((Conteo / sum(Conteo)) * 100, 2))
  
  tabla_latex <- kable(tabla, format = "latex", booktabs = TRUE, align = c("l", "c", "l")) %>%
    row_spec(0, background = '#87A96B', color = 'black', bold = TRUE)
  
  return(tabla_latex)
}


crear_grafica <- function(datos, columna){
  grafica <- ggplot(datos,aes(!!sym(columna))) +
    geom_bar( fill = rgb(0.53, 0.66, 0.42), colour = "white") + xlab("") + ylab("") 
	grafica + theme_bw()
	
return(grafica)	
    
}

crear_graficav <- function(datos, columna){
  grafica <- ggplot(datos,aes(!!sym(columna))) +
    geom_bar( fill = rgb(0.53, 0.66, 0.42), colour = "white") + xlab("") + ylab("") +
    coord_flip()
	grafica + theme_bw()
	
return(grafica)	
    
}


